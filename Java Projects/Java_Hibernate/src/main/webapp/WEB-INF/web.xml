<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <welcome-file-list>
  	<welcome-file>Homepage.html</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>Configuration</servlet-name>
  	<display-name>Configuration</display-name>
  	<description></description>
  	<servlet-class>ConfigurationServlet.Configuration</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Configuration_Salary</servlet-name>
  	<display-name>Configuration_Salary</display-name>
  	<description></description>
  	<servlet-class>ConfigurationServlet.Configuration_Salary</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Configuration_Current</servlet-name>
  	<display-name>Configuration_Current</display-name>
  	<description></description>
  	<servlet-class>ConfigurationServlet.Configuration_Current</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Loan_Account</servlet-name>
  	<display-name>Loan_Account</display-name>
  	<description></description>
  	<servlet-class>Open_Account.Loan_Account</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Configuration_Loan</servlet-name>
  	<display-name>Configuration_Loan</display-name>
  	<description></description>
  	<servlet-class>ConfigurationServlet.Configuration_Loan</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Configuration</servlet-name>
  	<url-pattern>/Configuration</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Configuration_Salary</servlet-name>
  	<url-pattern>/Configuration_Salary</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Configuration_Current</servlet-name>
  	<url-pattern>/Configuration_Current</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Loan_Account</servlet-name>
  	<url-pattern>/Loan_Account</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Configuration_Loan</servlet-name>
  	<url-pattern>/Configuration_Loan</url-pattern>
  </servlet-mapping>
</web-app>
